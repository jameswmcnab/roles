{
    "name": "ultraware/roles",
    "description": "Powerful package for handling roles and permissions in Laravel 5.",
    "keywords": [
        "roles",
        "permissions",
        "acl",
        "auth",
        "laravel",
        "illuminate"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ultraware"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ultraware\\Roles\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "orchestra/database": "^6.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "~9.3",
        "sebastian/environment": "^5.1",
        "php-coveralls/php-coveralls": "^2.3"
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/User.php",
            "tests/database/factories/UserFactory.php",
            "tests/database/factories/RoleFactory.php",
            "tests/database/factories/PermissionFactory.php"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ultraware\\Roles\\RolesServiceProvider"
            ]
        }
    }
}
